<launch>
    <arg
        name="video_bag_file"
        default="$(find orb_slam)/dataset/rgbd_dataset_freiburg1_rpy.bag"/>
    <node
        pkg="rosbag"
        type="play"
        name="player"
        args="$(arg video_bag_file)" />
    <rosparam
        command="load"
        file="$(find orb_slam)/tests/geometry/cfg/camera.yaml" />
    <test test-name="camera_tests" pkg="orb_slam" type="camera_tests" />
    <!-- launch-prefix="xterm -e /usr/bin/gdb -args" -->
</launch>